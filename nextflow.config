// This works to clean things up but I can't see 
// how to globally force copy or hardlinks for 
// published results so we don't just end up with
// a bunch of broken symlinks

// cleanup.enabled = true

process {

    executor = 'slurm'
    memory = 5.GB
    cpus = 2


    withLabel: bigMem {
        memory = 30.GB
    }

    withLabel: hugeMem{
    	memory = 30.GB
    }

    withLabel: multiCore {
        cpus = 8
    }

    withLabel: quadCore{
    	cpus = 4
   }
}